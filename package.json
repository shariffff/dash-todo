{
  "name": "dash-todo",
  "version": "0.0.1",
  "private": false,
  "homepage": "https://github.com/shariffff/dash-todo",
  "scripts": {
    "start": "wp-scripts start src/index.js --output-path=build",
    "hot": "wp-scripts start src/index.js --output-path=build --hot --allowed-hosts all",
    "build": "wp-scripts build src/index.js --output-path=build",
    "format": "wp-scripts format ./src",
    "plugin-zip": "wp-scripts plugin-zip",
    "env": "wp-env"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^6.26.0",
    "@wordpress/components": "^23.6.0",
    "@wordpress/element": "^5.6.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@wordpress/env": "^5.13.0",
    "@wordpress/scripts": "25.5.1"
  },
  "browserslist": "last 2 versions, Firefox ESR, not dead, not op_mini all, not ie <= 11"
}